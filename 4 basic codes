//LCM
int main(){
    int i;
    int n,m;
    scanf("%d",&n);
    scanf("%d",&m);
    if(n>m){
        i=n;
    }
    else{
        i=m;
    }
    while(i<=(n*m)){
        if((i%n==0) && (i%m==0)){
            break;}
            i++;
        }
             printf("%d",i);
       
    
    
}
