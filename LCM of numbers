//LCM of number
#include <stdio.h>
#include<math.h>
int lcm(int a,int b){
    int i;
    if(a>b){
        i=a;
    }
    else{
        i=b;
    }
    while(i<=(a*b)){
        if((i%a==0) && (i%b==0)){
            break;
            
        }
            i++;
        }
             printf("%d",i);
       
    
}
int main()
{
    
    int a,b;
    scanf("%d%d",&a,&b);
    lcm(a,b);
    return 0;
}
